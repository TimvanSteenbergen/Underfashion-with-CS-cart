  `destination_id` MEDIUMINT(8) UNSIGNED NOT NULL AUTO_INCREMENT,
  `localization` VARCHAR(255) NOT NULL DEFAULT '',
  `status` CHAR(1) NOT NULL DEFAULT 'A',
  PRIMARY KEY (`destination_id`),
